#include <stdio.h>

int main() {
    int arr[10] = {4, 8, 19, 3, 22, 11, 2, 17, 31, 10};
    int n = 10, i, j, temp;

    // Metode bubble sort
    for(i = 0; i < n-1; i++) {
        for(j = 0; j < n-i-1; j++) {
            if(arr[j] > arr[j+1]) {
                temp = arr[j];
                arr[j] = arr[j+1];
                arr[j+1] = temp;
            }
        }
    }

    printf("Bilangan setelah diurutkan: ");
    for(i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }
    return 0;
}
